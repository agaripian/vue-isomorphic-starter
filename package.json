{
  "private": true,
  "name": "saturday-project",
  "version": "1.0.0",
  "description": "Saturday Project",
  "scripts": {
    "heroku-postbuild": "npm run build",
    "build": "rimraf dist && npm run build:client && npm run build:server",
    "build:client": "cross-env NODE_ENV=production webpack --config lib/web/build/webpack.client.config.js --progress --hide-modules",
    "build:server": "cross-env NODE_ENV=production webpack --config lib/web/build/webpack.server.config.js --progress --hide-modules",
    "dev": "nodemon --watch lib/app.js --watch lib/core --watch lib/api --watch lib/web/auth.js --watch lib/web/resource.js --watch lib/web/build server.js",
    "lint": "eslint lib test",
    "test": "npm run lint && npm run test-cover && npm run test-check-coverage",
    "test-watch": "babel-node ./node_modules/.bin/_mocha --require should --recursive --reporter min --watch",
    "test-cover": "babel-node ./node_modules/.bin/babel-istanbul cover _mocha -- --require should --recursive",
    "test-check-coverage": "babel-istanbul check-coverage --statements 100 --functions 100 --branches 100 --lines 100"
  },
  "dependencies": {
    "auth0-js": "^8.8.0",
    "autoprefixer": "^7.1.1",
    "axios": "^0.16.1",
    "babel-core": "^6.24.1",
    "babel-loader": "^7.0.0",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-preset-env": "^1.5.1",
    "body-parser": "^1.17.2",
    "connect-ensure-login": "^0.1.1",
    "connect-redis": "^3.3.0",
    "cors": "^2.8.3",
    "cross-env": "^5.0.0",
    "css-loader": "^0.28.3",
    "dotenv-safe": "^4.0.4",
    "express": "^4.15.3",
    "express-async-wrap": "^1.0.0",
    "express-jwt": "^5.3.0",
    "express-session": "^1.15.3",
    "extract-text-webpack-plugin": "^2.1.0",
    "fast-async": "^6.2.2",
    "file-loader": "^0.11.1",
    "friendly-errors-webpack-plugin": "^1.6.1",
    "jwks-rsa": "^1.1.1",
    "namespaced-console-logger": "^1.0.1",
    "nodent-runtime": "^3.0.4",
    "passport": "^0.3.2",
    "passport-auth0": "^0.6.0",
    "pg": "^6.2.3",
    "rimraf": "^2.6.1",
    "url-loader": "^0.5.8",
    "vue": "^2.3.3",
    "vue-loader": "^12.2.0",
    "vue-router": "^2.5.3",
    "vue-server-renderer": "^2.3.3",
    "vue-style-loader": "^3.0.1",
    "vue-template-compiler": "^2.3.3",
    "vuex": "^2.3.1",
    "vuex-router-sync": "^4.1.3",
    "webpack": "^2.6.1",
    "webpack-merge": "^4.1.0",
    "webpack-node-externals": "^1.6.0"
  },
  "engines": {
    "node": "^8.0.0",
    "npm": "^5.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-istanbul": "^0.12.2",
    "eslint": "^3.19.0",
    "memory-fs": "^0.4.1",
    "mocha": "^3.4.2",
    "nodemon": "^1.11.0",
    "proxyquire": "^1.8.0",
    "require-directory": "^2.1.1",
    "should": "^11.2.1",
    "sinon": "^2.3.2",
    "webpack-dev-middleware": "^1.10.2",
    "webpack-hot-middleware": "^2.18.0"
  }
}
